<svg width="600" height="600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="600" height="600" fill="#0c4a6e" />
  
  <!-- Deep Ocean Background with Gradient -->
  <linearGradient id="oceanGradient" x1="0%" y1="0%" x2="100%" y2="100%">
    <stop offset="0%" stop-color="#0c4a6e" />
    <stop offset="100%" stop-color="#075985" />
  </linearGradient>
  <rect width="600" height="600" fill="url(#oceanGradient)" />
  
  <!-- Ocean Waves -->
  <path d="M0,400 C150,450 300,380 450,430 C550,465 600,400 600,400 L600,600 L0,600 Z" fill="#0284c7" opacity="0.7" />
  <path d="M0,450 C100,430 250,500 400,450 C550,400 600,450 600,450 L600,600 L0,600 Z" fill="#0ea5e9" opacity="0.5" />
  <path d="M0,500 C150,480 300,530 450,490 C550,470 600,500 600,500 L600,600 L0,600 Z" fill="#38bdf8" opacity="0.4" />
  
  <!-- NFT Frame -->
  <rect x="120" y="70" width="360" height="440" rx="20" fill="#082f49" stroke="#0c4a6e" stroke-width="8" />
  
  <!-- NFT Content (Ocean Scene) -->
  <rect x="140" y="90" width="320" height="320" rx="10" fill="#164e63" />
  
  <!-- Ocean Art -->
  <circle cx="300" cy="250" r="100" fill="#075985" />
  <circle cx="300" cy="250" r="80" fill="#0369a1" />
  <circle cx="300" cy="250" r="60" fill="#0284c7" />
  
  <!-- Stylized Wave Pattern -->
  <path d="M140,240 C180,220 220,260 260,240 C300,220 340,260 380,240 C420,220 460,260 460,240" 
        stroke="#38bdf8" stroke-width="8" fill="none" />
  <path d="M140,280 C180,260 220,300 260,280 C300,260 340,300 380,280 C420,260 460,300 460,280" 
        stroke="#0ea5e9" stroke-width="8" fill="none" />
  <path d="M140,320 C180,300 220,340 260,320 C300,300 340,340 380,320 C420,300 460,340 460,320" 
        stroke="#0284c7" stroke-width="8" fill="none" />
  
  <!-- Bubbles -->
  <circle cx="180" cy="150" r="15" fill="#7dd3fc" opacity="0.6" />
  <circle cx="400" cy="190" r="10" fill="#7dd3fc" opacity="0.4" />
  <circle cx="220" cy="190" r="8" fill="#7dd3fc" opacity="0.7" />
  <circle cx="370" cy="140" r="12" fill="#7dd3fc" opacity="0.5" />
  <circle cx="420" cy="300" r="10" fill="#7dd3fc" opacity="0.6" />
  <circle cx="200" cy="350" r="15" fill="#7dd3fc" opacity="0.4" />
  
  <!-- NFT Info -->
  <rect x="140" y="420" width="320" height="90" rx="10" fill="#075985" />
  <text x="160" y="450" font-family="Arial" font-size="24" fill="white" font-weight="bold">Ocean Dreams #42</text>
  <text x="160" y="480" font-family="Arial" font-size="16" fill="#38bdf8">BasedSea Collection</text>
  
  <!-- Price Tag -->
  <rect x="350" y="440" width="90" height="30" rx="15" fill="#065f46" opacity="0.7" />
  <text x="365" y="462" font-family="Arial" font-size="16" fill="#4ade80" font-weight="bold">2.5 ETH</text>
  
  <!-- BasedSea Logo -->
  <text x="480" y="580" font-family="Arial" font-size="16" fill="white" opacity="0.5" text-anchor="end">BasedSea</text>
</svg> 